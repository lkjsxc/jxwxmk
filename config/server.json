{
  "version": 1,
  "bind_http": "0.0.0.0:8080",
  "protocol_version": 3,
  "tick_rate": 30,
  "limits": {
    "ws_max_message_bytes": 16384,
    "ws_messages_per_sec": 30,
    "ws_burst": 60,
    "ws_idle_timeout_secs": 10,
    "ws_heartbeat_interval_secs": 5,
    "max_name_len": 24
  },
  "rate_limits": {
    "session_claim_per_ip_per_minute": 10
  }
}
