{
  "name": "starve-game-frontend",
  "version": "0.1.0",
  "description": "Frontend for Starve.io clone game",
  "main": "dist/main.js",
  "scripts": {
    "dev": "npx webpack-cli serve --mode development",
    "build": "npx webpack-cli --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "game",
    "typescript",
    "websocket",
    "canvas"
  ],
  "author": "",
  "license": "Apache-2.0"
}